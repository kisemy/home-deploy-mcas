<% if @morgage_updated.eql?(true) %>
  $('.modal').modal('close');
  swal({
      icon: "success",
      title: 'Success',
      buttons: false,
      timer: 3000,
      text: "<%= t('views.morgages.morgage_updated') %>"}).then(function(){
      window.location = '<%= morgage_path(@morgage) %>'
  });
<% else %>
  swal({
      icon: "error",
      title: 'Oops..!',
      buttons: false,
      timer: 3000,
      text: "<%= @morgage.errors.full_messages.join('.') %>"
  });
<% end %>
